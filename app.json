{
  "expo": {
    "name": "BBIOK_App",
    "slug": "BBIOK_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nmr.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "android",
      "web",
      "ios"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "no",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/nmr.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.khyunsu.BBIOK_App"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/nmr.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/nmr.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/3a8db303-b445-445e-9dd5-ea2257ed0dc3"      
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a8db303-b445-445e-9dd5-ea2257ed0dc3"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "owner": "khyunsu"
  }
}
